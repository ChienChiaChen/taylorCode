<?xml version="1.0" encoding="utf-8"?>
<test.taylor.com.taylorcode.ui.NumberRow
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/cootek_pre_call_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginStart="16dp"
        android:fontFamily="sans-serif-medium"
        android:lineSpacingExtra="-4sp"
        android:textColor="#383838"
        android:textSize="16sp"
        android:textStyle="normal" />

    <ImageView
        android:id="@+id/iv_pre_call_is_sc_user_mark"
        android:layout_width="44dp"
        android:layout_height="22dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="48dp"
        android:layout_centerVertical="true"
        android:src="@drawable/cootek_radiobutton_normal"
        android:visibility="gone" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_centerVertical="true"
        android:layout_alignParentEnd="true"
        android:duplicateParentState="true"
        android:src="?android:listChoiceIndicatorSingle" />

</test.taylor.com.taylorcode.ui.NumberRow>